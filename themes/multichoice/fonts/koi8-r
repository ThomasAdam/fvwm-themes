DestroyFunc FuncFvwmSetFont-WindowTitle
AddToFunc   FuncFvwmSetFont-WindowTitle
+ I Style "$0" Font "-cronyx-helvetica-bold-r-*-*-14-*-*-*-*-*-koi8-r"

DestroyFunc FuncFvwmSetFont-IconTitle
AddToFunc   FuncFvwmSetFont-IconTitle
+ I Style "$0" IconFont "-cronyx-helvetica-medium-r-*-*-11-*-*-*-*-*-koi8-r"

DestroyFunc FuncFvwmSetFont-Menu
AddToFunc   FuncFvwmSetFont-Menu
+ I MenuStyle "$0" Font "-cronyx-helvetica-medium-r-*-*-14-*-*-*-*-*-koi8-r"

DestroyFunc FuncFvwmSetFont-Module
AddToFunc   FuncFvwmSetFont-Module
+ I Nop
